<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.emiliedoyle.peer_mentoring_app.MenteeYourMatchesActivity">

    <TextView
        android:text="Your Matches"
        android:textColor="#9e64d8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/yourMatches"
        android:textStyle="bold"
        android:textSize="25dp"/>

    <TextView
        android:text="matches need volley"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/yourMatches"
        android:id="@+id/matchesvOLLEY"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/HomeButton"
        android:text="Return to Main Menu"
        android:layout_below="@+id/matchesvOLLEY"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
